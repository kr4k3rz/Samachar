<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_subscribe"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.codelite.kr4k3rz.samachar.ui.activity.SubscribeActivity">


    <include
        android:id="@+id/toolbar_subscribe"
        layout="@layout/toolbar_main"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />


    <android.support.v7.widget.RecyclerView
        android:id="@+id/recyclerView_subscribe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/toolbar_subscribe"
        android:scrollbars="vertical" />

</RelativeLayout>
